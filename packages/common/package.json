    "test:cover": "cross-env NODE_ENV=test nyc --reporter=html mocha --recursive --require @babel/register --require @babel/polyfill --colors \"./test/**/*.spec.js\""
  },
  "dependencies": {
    "bignumber.js": "^8.0.1",
    "bs58": "^4.0.1",
    "mipher": "^1.1.5"
