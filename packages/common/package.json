{
  "name": "@sbx/common",
  "version": "0.0.5",
  "description": "Core basics for the PascalCoin BlockChain project.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "PascalCoin",
    "BlockChain",
    "Cryptography"
  ],
  "author": "Benjamin Ansbach <benjaminansbach@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
